def vacuum_cleaner(location, status):
    print("Initial State:")
    print("Location:", location)
    print("Status:", status)

    # If current room is dirty, clean it
    if status[location] == 1:
        print("Action: SUCK")
        status[location] = 0
    else:
        print("Action: MOVE")

    # Move to other room
    if location == 'A':
        location = 'B'
    else:
        location = 'A'

    # Clean the second room if dirty
    if status[location] == 1:
        print("Action: SUCK")
        status[location] = 0

    print("\nFinal State:")
    print("Location:", location)
    print("Status:", status)

# Driver code
status = {'A': 1, 'B': 1}   # 1 = Dirty, 0 = Clean
location = 'A'

vacuum_cleaner(location, status)
