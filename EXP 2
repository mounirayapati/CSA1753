N = 8

# Check if placing queen is safe
def is_safe(board, row, col):
    for i in range(row):
        # Check column
        if board[i] == col:
            return False
        # Check diagonals
        if abs(board[i] - col) == abs(i - row):
            return False
    return True

# Solve using backtracking
def solve_queens(board, row):
    if row == N:
        print_solution(board)
        return True

    for col in range(N):
        if is_safe(board, row, col):
            board[row] = col
            if solve_queens(board, row + 1):
                return True
            board[row] = -1
    return False

# Print board
def print_solution(board):
    for i in range(N):
        for j in range(N):
            if board[i] == j:
                print("Q", end=" ")
            else:
                print(".", end=" ")
        print()

# Driver code
board = [-1] * N
solve_queens(board, 0)
